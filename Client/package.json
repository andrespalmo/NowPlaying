{
  "name": "nowplaying",
  "version": "1.0.0",
  "description": "The #NowPlaying page is a simple one-page app where visitors can visualize the most recent videos being shared on Twitter by people nearby",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joselito12344/NowPlaying.git"
  },
  "author": "Jose Ospina",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-material": "^1.1.0",
    "angular-ui-router": "^0.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.0",
    "eslint": "^3.3.1",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
